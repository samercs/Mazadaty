@model Mzayad.Web.Models.Order.DetailsViewModel

@{
    ViewBag.Title = "Order Received";
}

@section styles{
    <style>
        .alert {
            font-size: 16px;
        }
    </style>
}

@Html.StatusMessage(Global.OrderSuccessMessage)


@if (!string.IsNullOrEmpty(Model.RedirectUrl))
{
    @Html.StatusMessage(string.Format(Global.PrizeChanceMessage, Model.RedirectUrl))
}


@Html.Partial("OrderDetailsSummary", Model)

